<%= form_for(@question) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="questionform">
    <%= f.text_area :content, placeholder: "Compose new question...", class: "form-control" %>
    <%= f.text_area :morecontent, placeholder: "Got more to add?", class: "form-control" %>
		<%= f.select :tag_list, options_for_select(["Auto", "Finance", "Online Shopping", "Telecom", "Technology", "Travel"]) %></br></br>
  	<%= f.submit "Post", class: "btn btn-primary custom" %>
  </div>
		<% end %>